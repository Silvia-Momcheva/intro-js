function sumAndVAT(input) {
  let numbers = input.map(Number);
  let sum = numbers.reduce((acc, num) => acc + num, 0);
  let vat = sum * 0.2;
  let total = sum + vat;

  console.log(`Sum: ${sum}`);
  console.log(`VAT: ${vat}`);
  console.log(`Total: ${total}`);
}

sumAndVAT(["1.2", "2.30", "3.10"]);
